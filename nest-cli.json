{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/final_api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/final_api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/final_api",
  "projects": {
    "final_api": {
      "type": "application",
      "root": "apps/final_api",
      "entryFile": "main",
      "sourceRoot": "apps/final_api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/final_api/tsconfig.app.json"
      }
    },
    "student_service": {
      "type": "application",
      "root": "apps/student_service",
      "entryFile": "main",
      "sourceRoot": "apps/student_service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/student_service/tsconfig.app.json"
      }
    },
    "teacher_service": {
      "type": "application",
      "root": "apps/teacher_service",
      "entryFile": "main",
      "sourceRoot": "apps/teacher_service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/teacher_service/tsconfig.app.json"
      }
    },
    "constants": {
      "type": "library",
      "root": "libs/constants",
      "entryFile": "index",
      "sourceRoot": "libs/constants/src",
      "compilerOptions": {
        "tsConfigPath": "libs/constants/tsconfig.lib.json"
      }
    },
    "dtos": {
      "type": "library",
      "root": "libs/dtos",
      "entryFile": "index",
      "sourceRoot": "libs/dtos/src",
      "compilerOptions": {
        "tsConfigPath": "libs/dtos/tsconfig.lib.json"
      }
    }
  }
}